<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.mega.s1.board.notice.NoticeRepository">

	<insert id="boardInsert" parameterType="com.mega.s1.board.notice.NoticeVO" useGeneratedKeys="true" keyProperty="num">
		insert into notice values(#{num}, #{title}, #{writer}, #{contents}, now(), 0, 1)
	</insert>

</mapper>