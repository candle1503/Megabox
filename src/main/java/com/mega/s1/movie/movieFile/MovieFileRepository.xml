<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mega.s1.movie.movieFile.MovieFileRepository">

	<select id="movieFileSelect" parameterType="MovieVO" resultType="MovieFileVO">
		select * from movieFile where movieNum=#{movieNum}
	</select>
	

	<insert id="movieFileInsert" parameterType="MovieFileVO">
		insert into movieFile values(0, #{oriName}, #{movieNum}, #{fileName}, #{status}, #{detail})
	</insert>
</mapper>